{
  "version": 2,
  "builds": [
    {
      "src": "main.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api-docs",
      "dest": "/public/swagger.html"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/main.js"
    }
  ]
}
